{
  "name": "cors-browser",
  "version": "1.0.0",
  "description": "A browser that can bypass CORS restrictions",
  "main": "main.js",
  "scripts": {
    "dev": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "productName": "跨域浏览器",
    "appId": "cors.browser.lly",
    "directories": {
      "output": "build"
    },
    "mac": {
      "target": "dmg",
      "icon": "static/icon/cors.icns"
    },
    "dmg": {
      "backgroundColor": "#fff"
    }
  },
  "repository": "https://github.com/yaocoding007/cors-browser",
  "keywords": [
    "Electron",
    "browser",
    "cors"
  ],
  "author": "lly",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^29.0.1",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {}
}
